{
  "version": 2,
  "name": "dapp_backend",
  "builds": [
    { "src": "backend/server.js", "use": "@vercel/node" },
    {
      "src": "frontend/package.json",
      "use": "npm"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "backend/server.js" }],
  "postbuild": "cd frontend && npm install && npm run build",
}
